l-form-item>\r\n            <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n        <el-table :data=\"tableData\" border style=\"width: 100%\">\r\n          <el-table-column fixed type=\"index\" label=\"序号\" width=\"60\"></el-table-column>\r\n          <el-table-column prop=\"bidId\" label=\"竞价ID\" width=\"80\"></el-table-column>\r\n          <el-table-column prop=\"bidAmount\" label=\"竞拍价\" width=\"60\"></el-table-column>\r\n          <el-table-column prop=\"bidTime\" label=\"竞价时间\"></el-table-column>\r\n          <el-table-column prop=\"auctionItem.auction.title\" label=\"承办方\"></el-table-column>\r\n          <el-table-column prop=\"auctionItem.item.seller.username\" label=\"物主\"></el-table-column>\r\n          <el-table-column prop=\"auctionItem.item.name\" label=\"商品名\"></el-table-column>\r\n          <el-table-column prop=\"bidder.username\" label=\"竞拍人\"></el-table-column>\r\n          <el-table-column prop=\"status\" label=\"状态\"></el-table-column>\r\n          <el-table-column prop=\"auctionItem.item.imageUrl\" label=\"图片\">\r\n            <template slot-scope=\"scope\">\r\n              <img :src=\"scope.row.auctionItem.item.imageUrl\" style=\"width: 100%; height: auto\" />\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"操作\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button @click=\"handleClick(scope.row)\" type=\"text\" size=\"small\">详情</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <el-dialog :visible.sync=\"detailsDialogVisible\" title=\"拍品详情\" width=\"70%\">\r\n          <div v-if=\"selectedItem\">\r\n            <p>竞价ID: {{ selectedItem.bidId }}</p>\r\n    <p>承办方: {{ selectedItem.auctionItem.auction.title }}</p>\r\n    <p>承办方描述: {{ selectedItem.auctionItem.auction.description }}</p>\r\n    <p>拍卖开始日期: {{ selectedItem.auctionItem.auction.startDate }}</p>\r\n    <p>拍卖结束日期: {{ selectedItem.auctionItem.auction.endDate }}</p>\r\n    <p>拍卖状态: {{ selectedItem.auctionItem.auction.status }}</p>\r\n    <p>物品ID: {{ selectedItem.auctionItem.item.itemId }}</p>\r\n    <p>物主: {{ selectedItem.auctionItem.item.seller.username }}</p>\r\n    <p>物主邮箱: {{ selectedItem.auctionItem.item.seller.email }}</p>\r\n    <p>物主联系方式: {{ selectedItem.auctionItem.item.seller.phoneNumber }}</p>\r\n    <p>物主地址: {{ selectedItem.auctionItem.item.seller.address }}</p>\r\n    <p>商品名称: {{ selectedItem.auctionItem.item.name }}</p>\r\n    <p>商品描述: {{ selectedItem.auctionItem.item.description }}</p>\r\n    <p>商品类别: {{ selectedItem.auctionItem.item.category }}</p>\r\n    <p>起拍价: {{ selectedItem.auctionItem.item.startingPrice }}</p>\r\n    <p>上市日期: {{ selectedItem.auctionItem.item.listingDate }}</p>\r\n    <p>当前价格: {{ selectedItem.auctionItem.item.nowPrice }}</p>\r\n    <p>保留价格: {{ selectedItem.auctionItem.reservePrice }}</p>\r\n    <p>成交价格: {{ selectedItem.auctionItem.soldPrice }}</p>\r\n    <p>销售状态: {{ selectedItem.auctionItem.saleStatus }}</p>\r\n    <p>竞拍人: {{ selectedItem.bidder.username }}</p>\r\n    <p>竞拍价格: {{ selectedItem.bidAmount }}</p>\r\n    <p>竞拍时间: {{ selectedItem.bidTime }}</p>\r\n    <img v-if=\"selectedItem.auctionItem.item.imageUrl\" :src=\"selectedItem.auctionItem.item.imageUrl\" alt=\"商品图片\" style=\"width: 100%; height: auto\">\r\n            <!-- Add more details display here as needed -->\r\n          </div>\r\n        </el-dialog>\r\n        <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[10, 20, 50, 100]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n        ></el-pagination>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import Axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        formInline: {\r\n          bidId: '',\r\n          type: '',\r\n\r\n        },\r\n        tableData: [],\r\n        currentPage: 1,\r\n        pageSize: 10,\r\n        total: 0,\r\n        detailsDialogVisible: false,\r\n        selectedItem: null\r\n      };\r\n    },\r\n    created() {\r\n      this.getdate(this.currentPage, this.pageSize);\r\n    },\r\n    methods: {\r\n      handleSizeChange(newSize) {\r\n        this.pageSize = newSize;\r\n        this.getdate(this.currentPage, this.pageSize);\r\n      },\r\n      handleCurrentChange(newPage) {\r\n        this.currentPage = newPage;\r\n        this.getdate(this.currentPage, this.pageSize);\r\n      },\r\n      onSubmit() {\r\n    console.log(this.formInline);\r\n    this.getdate(this.currentPage, this.pageSize);\r\n\r\n      },\r\n      async getdate(page, pageSize) {\r\n        try {\r\n          const response = await Axios.get('http://localhost:8080/getbids', {\r\n            params: {\r\n              bidId: this.formInline.bidId,\r\n              status: this.formInline.type,\r\n              page: page,\r\n              pageSize: pageSize\r\n            }\r\n          });\r\n          this.tableData = response.data.data.rows;\r\n          this.total = response.data.data.total;\r\n        } catch (error) {\r\n          console.error('Error fetching data:', error);\r\n          this.$message.error('获取数据失败，请重试');\r\n        }\r\n      },\r\n      handleClick(item) {\r\n        this.selectedItem = item;\r\n        this.detailsDialogVisible = true;\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Add any specific styles for your component here */\r\n  </style>\r\n  "]}]}
