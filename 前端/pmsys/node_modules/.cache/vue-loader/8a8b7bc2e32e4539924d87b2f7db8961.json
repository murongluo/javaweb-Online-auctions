     <el-submenu :index=\"item.index\" :key=\"item.index\">\r\n                        <template slot=\"title\">\r\n                            <i :class=\"item.icon\"></i>\r\n                            <span slot=\"title\">{{ item.title }}</span>\r\n                        </template>\r\n                        <template v-for=\"subItem in item.subs\">\r\n                            <el-submenu v-if=\"subItem.subs\" :index=\"subItem.index\" :key=\"subItem.index\">\r\n                                <template slot=\"title\">{{ subItem.title }}</template>\r\n                                <el-menu-item v-for=\"(threeItem, i) in subItem.subs\" :key=\"i\" :index=\"threeItem.index\">{{\r\n                                    threeItem.title\r\n                                }}</el-menu-item>\r\n                            </el-submenu>\r\n                            <el-menu-item v-else :index=\"subItem.index\" :key=\"subItem.index\">{{ subItem.title }}</el-menu-item>\r\n                        </template>\r\n                    </el-submenu>\r\n                </template>\r\n                <template v-else>\r\n                    <el-menu-item :index=\"item.index\" :key=\"item.index\">\r\n                        <i :class=\"item.icon\"></i>\r\n                        <span slot=\"title\">{{ item.title }}</span>\r\n                    </el-menu-item>\r\n                </template>\r\n            </template>\r\n        </el-menu>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport bus from '../common/bus';\r\nexport default {\r\n    data() {\r\n        return {\r\n            collapse: false,\r\n            items: [\r\n                {\r\n                    icon: 'el-icon-lx-home',\r\n                    index: 'dashboard',\r\n                    title: '系统首页'\r\n                },\r\n             \r\n                {\r\n                    icon: 'el-icon-lx-calendar',\r\n                    index: '4',\r\n                    title: '管理',\r\n                    subs: [\r\n                        {\r\n                            index: '3-1',\r\n                            title: '用户管理',\r\n                            subs: [\r\n                                {\r\n                                    index: 'userm',\r\n                                    title: '用户管理'\r\n                                }\r\n                            ]\r\n                        },\r\n                        {\r\n                            index: '3-2',\r\n                            title: '拍品管理',\r\n                            subs: [\r\n                                {\r\n                                    index: 'itemm',\r\n                                    title: '拍品管理'\r\n                                }\r\n                            ]\r\n                        },\r\n                        {\r\n                            index: '3-3',\r\n                            title: '拍卖管理',\r\n                            subs: [\r\n                                {\r\n                                    index: 'bidm',\r\n                                    title: '竞价管理'\r\n                                },\r\n                                {\r\n                                    index: 'chartsm',\r\n                                    title: '数据分析'\r\n                                },\r\n                                {\r\n                                    index: 'auctionm',\r\n                                    title: '拍卖会管理'\r\n                                },\r\n                                {\r\n                                    index: 'auitm',\r\n                                    title: '拍卖管理'\r\n                                },\r\n                                {\r\n                                    index: 'paym',\r\n                                    title: '支付订单'\r\n                                }\r\n                            ]\r\n                        }\r\n                    ]\r\n                },\r\n                \r\n            ]\r\n        };\r\n    },\r\n    computed: {\r\n        onRoutes() {\r\n            return this.$route.path.replace('/', '');\r\n        }\r\n    },\r\n    created() {\r\n        // 通过 Event Bus 进行组件间通信，来折叠侧边栏\r\n        bus.$on('collapse', (msg) => {\r\n            this.collapse = msg;\r\n            bus.$emit('collapse-content', msg);\r\n        });\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar {\r\n    display: block;\r\n    position: absolute;\r\n    left: 0;\r\n    top: 70px;\r\n    bottom: 0;\r\n    overflow-y: scroll;\r\n}\r\n.sidebar::-webkit-scrollbar {\r\n    width: 0;\r\n}\r\n.sidebar-el-menu:not(.el-menu--collapse) {\r\n    width: 250px;\r\n}\r\n.sidebar > ul {\r\n    height: 100%;\r\n}\r\n</style>\r\n"]}]}
