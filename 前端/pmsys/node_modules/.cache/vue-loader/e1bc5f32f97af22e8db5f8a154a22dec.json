l-col :span=\"24\">\r\n          <el-input\r\n            v-model=\"searchQuery\"\r\n            placeholder=\"搜索商品\"\r\n            clearable\r\n            @keyup.enter.native=\"handleSearch\"\r\n          >\r\n            <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"handleSearch\"></el-button>\r\n          </el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"24\">\r\n          <el-card class=\"box-card\">\r\n            <div class=\"content\">\r\n              <el-row :gutter=\"20\">\r\n                <el-col :span=\"8\" v-for=\"(product, index) in filteredProducts\" :key=\"index\">\r\n                  <el-card class=\"box-card\">\r\n                    <img :src=\"product.image\" class=\"image\" alt=\"商品图片\">\r\n                    <div class=\"content\">\r\n                      <span class=\"name\">{{ product.name }}</span>\r\n                      <br>\r\n                      <span class=\"description\">{{ product.description }}</span>\r\n                      <br>\r\n                      <span class=\"price\">当前价格：{{ product.price }}</span>\r\n                    </div>\r\n                    <div class=\"bottom clearfix\">\r\n                      <el-button type=\"text\" class=\"button\" @click=\"viewProduct(product)\">查看</el-button>\r\n                      <el-button type=\"text\" class=\"button\" @click=\"addToCart(product)\">竞价</el-button>\r\n                    </div>\r\n                  </el-card>\r\n                </el-col>\r\n              </el-row>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\" style=\"margin-top: 20px;\">\r\n        <el-col :span=\"24\">\r\n          <el-pagination\r\n            background\r\n            layout=\"prev, pager, next\"\r\n            :total=\"totalProducts\"\r\n            :page-size=\"pageSize\"\r\n            @current-change=\"handlePageChange\"\r\n          ></el-pagination>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        searchQuery: '',\r\n        products: [\r\n          // 示例商品数据\r\n          { id: 1, image: 'https://via.placeholder.com/150', name: '商品1', description: '商品描述1', price: '¥100' },\r\n          { id: 2, image: 'https://via.placeholder.com/150', name: '商品2', description: '商品描述2', price: '¥200' },\r\n          // ...更多商品\r\n        ],\r\n        totalProducts: 100, // 总商品数\r\n        pageSize: 8, // 每页显示的商品数\r\n        currentPage: 1, // 当前页码\r\n      };\r\n    },\r\n    computed: {\r\n      filteredProducts() {\r\n        if (!this.searchQuery) {\r\n          return this.products;\r\n        }\r\n        return this.products.filter(product =>\r\n          product.name.includes(this.searchQuery) ||\r\n          product.description.includes(this.searchQuery)\r\n        );\r\n      }\r\n    },\r\n    methods: {\r\n      viewProduct(product) {\r\n        // 查看商品的逻辑\r\n        console.log('查看商品:', product);\r\n      },\r\n      addToCart(product) {\r\n        // 添加商品到购物车的逻辑\r\n        console.log('添加商品到购物车:', product);\r\n      },\r\n      handleSearch() {\r\n        this.currentPage = 1; // 搜索时重置当前页码\r\n      },\r\n      handlePageChange(newPage) {\r\n        this.currentPage = newPage;\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  /* 淘宝商城风格样式 */\r\n  .el-row {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .el-col {\r\n    border-radius: 4px;\r\n  }\r\n  \r\n  .box-card {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .image {\r\n    width: 100%;\r\n    display: block;\r\n  }\r\n  \r\n  .content {\r\n    padding: 14px;\r\n    font-size: 14px;\r\n    line-height: 20px;\r\n    color: #303133;\r\n    word-wrap: break-word;\r\n  }\r\n  \r\n  .name {\r\n    font-size: 16px;\r\n    color: #303133;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .description {\r\n    color: #909399;\r\n    font-size: 14px;\r\n  }\r\n  \r\n  .price {\r\n    color: #f56c6c;\r\n    font-size: 14px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .bottom {\r\n    margin-top: 13px;\r\n    line-height: 12px;\r\n  }\r\n  \r\n  .button {\r\n    padding: 0;\r\n    margin-left: 5px;\r\n  }\r\n  </style>\r\n  "]}]}
